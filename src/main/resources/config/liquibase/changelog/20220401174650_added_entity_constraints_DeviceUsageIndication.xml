<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <!--
        Added the constraints for entity DeviceUsageIndication.
    -->
    <changeSet id="20220401174650-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="fda_submission_id"
                                 baseTableName="device_usage_indication"
                                 constraintName="fk_device_usage_indication__fda_submission_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fda_submission"/>

        <addForeignKeyConstraint baseColumnNames="alteration_id"
                                 baseTableName="device_usage_indication"
                                 constraintName="fk_device_usage_indication__alteration_id"
                                 referencedColumnNames="id"
                                 referencedTableName="alteration"/>

        <addForeignKeyConstraint baseColumnNames="cancer_type_id"
                                 baseTableName="device_usage_indication"
                                 constraintName="fk_device_usage_indication__cancer_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cancer_type"/>

        <addForeignKeyConstraint baseColumnNames="drug_id"
                                 baseTableName="device_usage_indication"
                                 constraintName="fk_device_usage_indication__drug_id"
                                 referencedColumnNames="id"
                                 referencedTableName="drug"/>
    </changeSet>
</databaseChangeLog>
